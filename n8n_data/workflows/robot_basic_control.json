      "parameters": {},
      "id": "manual-trigger",
      "name": "Manual Trigger",
      "type": "n8n-nodes-base.manualTrigger",
      "typeVersion": 1,
      "position": [20, 300]
    },
    {
      "parameters": {
        "values": {
          "string": [
            {
              "name": "command",
              "value": "={{ (() => {\n  const cmd = ($json.body.command || 'forward').toLowerCase().trim();\n  // Map common input variations to valid commands\n  const commandMap = {\n    'forward': 'forward',\n    'move forward': 'forward',\n    'go forward': 'forward',\n    'backward': 'backward',\n    'move backward': 'backward',\n    'go backward': 'backward',\n    'back': 'backward',\n    'move back': 'backward',\n    'go back': 'backward',\n    'left': 'left',\n    'turn left': 'left',\n    'go left': 'left',\n    'right': 'right',\n    'turn right': 'right',\n    'go right': 'right',\n    'stop': 'stop',\n    'halt': 'stop',\n    'emergency stop': 'stop'\n  };\n  return commandMap[cmd] || 'forward';\n})() }}"
            }
          ]
        }
      },
      "id": "command-setter",
      "name": "Set Command",
      "type": "n8n-nodes-base.set",
      "typeVersion": 1,
      "position": [240, 300]
    },
    {
      "parameters": {
        "conditions": {
          "string": [
            {
              "value1": "={{ $json.command }}",
              "operation": "equal",
              "value2": "forward"
            }
          ]
        }
      },
      "id": "forward-condition",
      "name": "Forward?",
      "type": "n8n-nodes-base.if",
      "typeVersion": 1,
      "position": [460, 200]
