        <collision name="collision">
          <geometry>
            <cylinder>
              <radius>0.05</radius>
              <length>0.8</length>
            </cylinder>
          </geometry>
        </collision>
      </link>

      <link name="table_leg3">
        <pose frame="">0.45 -0.45 -0.4 0 0 0</pose>
        <visual name="visual">
          <geometry>
            <cylinder>
              <radius>0.05</radius>
